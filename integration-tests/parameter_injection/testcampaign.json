{
	"buildCommand": "cp testcampaign.ah testproject/ && cd testproject && ag++ -std=c++98 -g main.cc ; rm -Rf testcampaign.ah",
	"experimentCommand": "./experimentCommand.sh",
	"logLevel": 3,
	"interfaces": [
		{
			"header_files": ["stdio.h"],
			"targets": [
				{
					"id": "testinjection",
					"signature": "int test(int a);",
					"injectAt": "argument",
					"argNumber": 0,
					"error_situations": [
						{
							"error_value": "42"
						}
					]
				}
			]
		}
	],
	"experiments": [
		{"target": "testinjection", "experiment_mode": "call_count", "callCount": 1, "injection_mode": "replace", "error_situation_index": 0}
	]
}
